<Window x:Class="MangaReader.VersionHistory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:properties="clr-namespace:MangaReader.Properties"
        Title="{x:Static properties:Strings.Update_Title}" Height="400" Width="500"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Icon="../main.ico">
    <Grid>
        <TextBox x:Name="TextBox" Margin="10,10,9.6,41" TextWrapping="Wrap" Text="TextBox"/>
        <Button Content="Ok" Margin="0,0,9.6,10" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="Ok_Click" IsDefault="True"/>
        <StatusBar Margin="10,0,102.6,10" RenderTransformOrigin="0.168,-1.134" Height="26" VerticalAlignment="Bottom" >
            <StatusBarItem HorizontalContentAlignment="Center">
                <TextBlock x:Name="Label"/>
            </StatusBarItem>
        </StatusBar>

    </Grid>
</Window>
