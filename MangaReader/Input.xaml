<Window x:Class="MangaReader.Input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Properties="clr-namespace:MangaReader.Properties"
        Icon="main.ico"
        Title="{x:Static Properties:Strings.Input_Title}" Height="107.333" Width="220.8"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        WindowStyle="ToolWindow">
    <StackPanel Margin="0,0,-0.4,0.4">
        <TextBlock Text="{x:Static Properties:Strings.Input_Text}" Margin="10,0,9.4,0" />
        <TextBox x:Name="Result" Margin="10,0,9.4,0" />
        <Button Content="{x:Static Properties:Strings.Input_Add}" Click="Add_click" IsDefault="True" Height="23" Margin="10,0,9.4,0"/>
    </StackPanel>
</Window>